{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://taskbuddy-65qv.onrender.com/api/v1/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/old-route",
      "destination": "/new-route",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
